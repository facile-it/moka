{
  "name": "facile-it/moka",
  "description": "Shorthand for creating mock objects",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "testing",
    "testing-tools",
    "mock",
    "mocking",
    "mocking-library",
    "phpunit"
  ],
  "homepage": "http://github.com/facile-it/moka",
  "authors": [
    {
      "name": "Alberto Villa",
      "email": "alberto.villa@facile.it"
    },
    {
      "name": "Angelo Giuffredi",
      "email": "angelo.giuffredi@facile.it"
    }
  ],
  "autoload": {
    "psr-4": {
      "Moka\\": "src/Moka/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.0",
    "phpcollection/phpcollection": "^0.5.0",
    "phpspec/prophecy": "^1.6",
    "phpunit/phpunit": "^5.7",
    "psr/container": "^1.0",
    "mockery/mockery": "^0.9.9"
  },
  "require-dev": {
    "facile-it/facile-coding-standard": "dev-master",
    "facile-it/paraunit": "^0.8.2"
  }
}
